{
  "name": "@socialgouv/meditor",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "description": "MDX-based Editor and Previewer for React.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "yarn build --watch",
    "dev": "concurrently \"yarn build:watch\" \"yarn serve\"",
    "serve": "http-server ./",
    "test": "echo \"Error: no test specified\" && exit 1",
    "preversion": "yarn build && git add -f ./dist/index.js",
    "postversion": "git push origin HEAD && git push --tags"
  },
  "keywords": [
    "editor",
    "markdown",
    "md",
    "mdx",
    "react"
  ],
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@mdx-js/mdx": "^1.1.6",
    "@mdx-js/react": "^1.1.6",
    "@mdx-js/tag": "0.20.3",
    "@socialgouv/eslint-config-react": "0.9.3",
    "concurrently": "4.1.1",
    "coveralls": "3.0.5",
    "eslint": "6.1.0",
    "http-server": "0.11.1",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "remark": "11.0.1",
    "remark-mdx": "1.1.6",
    "remark-parse": "7.0.1",
    "remark-react": "6.0.0",
    "rollup": "1.18.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.1",
    "rollup-plugin-filesize": "6.1.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "showdown": "1.9.0",
    "unified": "8.3.2"
  },
  "resolutions": {
    "ecstatic": "3.0.0"
  }
}
