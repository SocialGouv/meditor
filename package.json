    "remark-mdx": "1.5.1",
    "remark-parse": "7.0.2",
    "remark-preset-lint-recommended": "3.0.3",
    "remark-react": "6.0.0",
    "remixicon": "2.1.0",
    "rollup": "1.27.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.1.0",
    "rollup-plugin-filesize": "6.2.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-livereload": "1.0.4",
