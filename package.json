{
  "name": "react-meditor",
  "version": "0.1.0-alpha.5",
  "license": "Apache-2.0",
  "description": "MDX-based Editor and Previewer for React.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">= 10.16.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "yarn build --watch",
    "dev": "cross-env NODE_ENV=development yarn build:watch",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint ./src/**",
    "test:unit": "jest",
    "test:watch": "yarn test:unit --watch",
    "preversion": "yarn test && yarn build",
    "postversion": "git push origin HEAD && git push --tags"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@mdx-js/mdx": "1.5.0",
    "@mdx-js/react": "1.5.0",
    "@mdx-js/tag": "0.20.3",
    "@socialgouv/eslint-config-react": "0.10.0",
    "@types/jest": "24.0.18",
    "@types/react": "16.9.4",
    "babel-jest": "24.9.0",
    "coveralls": "3.0.6",
    "cross-env": "6.0.0",
    "eslint": "6.5.0",
    "jest": "24.9.0",
    "postcss-url": "8.0.0",
    "prettier": "1.18.2",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "remark": "11.0.1",
    "remark-mdx": "1.5.0",
    "remark-parse": "7.0.1",
    "remark-react": "6.0.0",
    "rollup": "1.22.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-copy": "3.1.0",
    "rollup-plugin-filesize": "6.2.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-livereload": "1.0.3",
    "rollup-plugin-node-builtins": "2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-serve": "1.0.1",
    "rollup-plugin-terser": "5.1.2",
    "rorre": "1.2.0",
    "showdown": "1.9.0",
    "unified": "8.4.0"
  },
  "peerDependencies": {
    "react": ">=16.10.1",
    "react-dom": ">=16.10.1"
  },
  "resolutions": {
    "bl": ">=0.9.5 <1.0.0 || >=1.0.1",
    "semver": ">=4.3.2"
  },
  "keywords": [
    "editor",
    "markdown",
    "md",
    "mdx",
    "react"
  ]
}
