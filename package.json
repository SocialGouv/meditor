    "@mdx-js/react": "1.5.1",
    "@mdx-js/tag": "0.20.3",
    "@socialgouv/eslint-config-react": "0.10.0",
    "@types/jest": "24.0.23",
    "@types/react": "16.9.13",
    "babel-jest": "24.9.0",
    "coveralls": "3.0.8",
    "cross-env": "6.0.3",
    "debounce": "1.2.0",
    "eslint": "6.7.1",
    "jest": "24.9.0",
    "keep-a-changelog": "0.8.1",
    "postcss-url": "8.0.0",
    "prettier": "1.19.1",
    "ramda": "0.26.1",
    "react": "16.12.0",
